{
  "name": "netlify-cms-site",
  "version": "1.0.0",
  "description": "Netlify CMS documentation website built with Gatsby",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build && rm -rf dist && mv public dist",
    "lint": "markdownlint 'content/docs/**/*.md'",
    "copy:contribs": "cp ../.all-contributorsrc data/contributors.json",
    "prepare": "npm run copy:contribs",
    "reset": "rm -rf .cache"
  },
  "author": "",
  "homepage": "https://www.netlifycms.org/",
  "license": "MIT",
  "dependencies": {
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "dayjs": "^1.8.23",
    "emotion-theming": "^10.0.27",
    "gatsby": "3.7.1",
    "gatsby-plugin-catch-links": "3.7.1",
    "gatsby-plugin-emotion": "^4.0.0",
    "gatsby-plugin-manifest": "3.7.1",
    "gatsby-plugin-netlify-cms": "5.7.1",
    "gatsby-plugin-react-helmet": "4.7.1",
    "gatsby-remark-autolink-headers": "4.4.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-prismjs": "5.4.1",
    "gatsby-source-filesystem": "3.7.1",
    "gatsby-transformer-json": "3.7.1",
    "gatsby-transformer-remark": "4.4.1",
    "gatsby-transformer-yaml": "3.7.1",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "netlify-cms-app": "^2.15.19",
    "prismjs": "^1.21.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-github-btn": "^1.1.1",
    "react-helmet": "^6.0.0",
    "react-markdown": "^6.0.0",
    "smooth-scroll": "^16.1.2"
  },
  "devDependencies": {
    "babel-plugin-prismjs": "^2.0.1",
    "babel-preset-gatsby": "^1.0.0",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.20.1",
    "markdownlint-cli": "^0.27.1"
  },
  "private": true
}

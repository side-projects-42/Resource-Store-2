<%="Listing Firewall".bold%>
<%index=1%>

<%firewalls.each do |firewall|%>
  <%="Firewall: #{index}".bold%>
    <%="Rule Name".fix(20)%>: <%=firewall[:rule] %>
    <%="Start IP Address".fix(20)%>: <%=firewall[:start_ip_address]%>
    <%="End IP Address".fix(20)%>: <%=firewall[:end_ip_address] %>   <%index+=1%>
<%end%>

.latest-metric(data-bind='text: latestValue')
.metric-change.year-over-year
    span.dates(data-bind='html: yearDates')
    span.value(data-bind='text: yearValue, css:yearCssClass')
.metric-change.month-over-month
    span.dates(data-bind='html: monthDates')
    span.value(data-bind='text: monthValue, css:monthCssClass')
